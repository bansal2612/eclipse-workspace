package com.testNG;

import org.testng.annotations.Test;

public class CoreJavaCpding {

	
	@Test
	public int sumOfElements(int arr[])
	{
		int sum=0;
		for(int i=0;i<arr.length;i++)
		{
			sum =sum+arr[i];
		}
		return sum;
	}
	
	@Test
	public static void multiply(int n)
	{
		int mul=0;
		for(int i=1;i<=10;i++)
		{
			mul=mul+n;
			System.out.println("Table of " + n + " is: " + mul);			
		}
	}
	public static int maxDiff()
	{
		int diff=0;
		int[] arr= {1,2,4,8,98,6,33,11,56,78,1,90};
		for(int i=0;i<arr.length-1;i++)
		{
			if(diff<(Math.abs(arr[i+1]-arr[i])))
			            diff=Math.abs(arr[i+1]-arr[i]);
			System.out.println(diff);
		
		}
		return diff;
	}
	
	public static void  sortAsc()
	{
		int[] arr= {1,2,4,8,98,6,33,11,56,78,1,90};
		int temp=0;
		for(int i=0;i<arr.length-1;i++)
		{
			for(int j=i+1;j<arr.length;j++)
			{
				if(arr[i]<arr[j])
				{
					temp=arr[i];
					arr[i]=arr[j];
					arr[j]=temp;
				}
			}
		}
		for(int i=0;i<arr.length;i++)
		{
			System.out.println(arr[i]);
		}
	}
	
	public static void swap()
	{
		int a=90;
		int b=80;
		a=a+b;
		b=a-b;
		a=a-b;
		System.out.println(a);
		System.out.println(b);
		
	}
	
	public static void printPyramid() {
		for(int i=0;i<5;i++)
		{
			for(int k=5-1 ;k>i;k--)
			{
				System.out.print(" ");
			}
			for(int j=0;j<2*i+1;j++)
			{
				System.out.print("*");
			}
			System.out.println("");
		}
	}
	
	public static boolean checkPrime(int n)
	{
		for(int i=2;i<n/2;i++)
		{
			if(n%i==0)
			{
				return false;
			}
			
		}
		return true;
	}
	
	
	public static void main(String[] args) {
		boolean flag=checkPrime(32);
		System.out.println(flag);
	}

}