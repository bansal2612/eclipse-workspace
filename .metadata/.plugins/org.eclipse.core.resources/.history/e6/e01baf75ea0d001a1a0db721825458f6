package adobeArray;

import org.testng.annotations.Test;

public class adobeArray {

public void findMax()
{
int[] a= {-1,-1,-2,0};
int max=a[0];
for(int i=0;i<a.length;i++)
{
	if(a[i]>max)
	{
		max=a[i];
	}
}
System.out.println(max);
}
@Test
public void checkArray()
{
boolean arr[]=new boolean[10];
int i=0;
while(i<arr.length)
{
System.out.println(arr[i]);

}
}


}
